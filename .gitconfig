[branch]
	verbose = true
[user]
	email = 125358469+PhilippFeO@users.noreply.github.com
	name = PhilippFeO
[log]
	date = relative
	; date = short
[format]
	; >(N) Padding links einfÃ¼gen
	; <(N) rechts
	pretty = format:%C(auto)%h %Cblue%>(12)%ad %Cgreen%<(7)%aN %Creset%s %C(auto)%d 
[alias]
	ac = commit -am
	am = commit --amend --no-edit
	bd = branch -d
	bdr = branch -dr
	co = checkout
	fo = fetch origin
	lg = log --graph
	od = co origin/dev
	pullom = pull origin main
	pushom = push origin main
	pmd = push origin main main:dlr
	unstage = restore --staged
	us = unstage
	ps = push
	pfwl = push --force-with-lease
[init]
	defaultBranch = main
[status]
	submoduleSummary = 1
[core]
	editor = nvim
